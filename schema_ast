- !ls | cat && rm


 	      |
	    /	\
           ls	 &&
         /       /  \
       !      cat    rm
 



- !ls | cat; env |grep && ls || pwd | less


	|
      /   \
    ls      ;
  /       /   \
!       cat    |
	      /  \
           env    &&
                 /   \
              grep    ||
                    /    \
                   ls     |
		        /   \
		      pwd    less
